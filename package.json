{
  "name": "gearwfm-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.5.1 <21.0.0",
    "bun": "*",
    "npm": "please-use-bun",
    "yarn": "please-use-bun"
  },
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:local": "cross-env LOCAL_IP=\"10.0.0.18\" next dev -H 10.0.0.18",
    "dev:local2": "cross-env LOCAL_IP=\"$(ifconfig $(route -n get default | awk '/interface: / {print $2}') | awk '/inet / {print $2}')\" next dev -H $(ifconfig $(route -n get default | awk '/interface: / {print $2}') | awk '/inet / {print $2}')",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.3.0",
    "@clerk/themes": "^2.1.20",
    "@hookform/devtools": "^4.3.1",
    "@hookform/resolvers": "^3.9.0",
    "@internationalized/date": "^3.5.5",
    "@nextui-org/react": "^2.4.6",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@uploadthing/react": "^6.7.2",
    "axios": "^1.7.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.30",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.428.0",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "nodemailer": "^6.9.14",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0-rc2",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.3.0",
    "react-papaparse": "^4.4.0",
    "sonner": "^1.5.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.2",
    "uploadthing": "^6.13.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
